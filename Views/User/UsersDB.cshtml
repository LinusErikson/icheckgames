
@{
    ViewBag.Title = "UsersDB";
}

<h2>UsersDB</h2>


<div class="container">
<div class="col-lg-12 col-sm-4 col-xs-6">
    
    <ul class="list-group">

        @foreach (var item in ViewBag.AllUsers)
        {
             <a href="/Profile/@item.Username"><li class="list-group-item">@item.Username </li> </a>
        }


    </ul>
    </div>
    </div>
